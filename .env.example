FAL_API_KEY=key

BOT_USERNAME="reve_image_bot"
REQUIRED_CHANNELS="@reve_ai_test"
TELEGRAM_SUPPORT_USERNAME="avato_memory_help"

# S3
STORAGE_TYPE=local
S3_REGION=ru-central1
S3_ENDPOINT=https://storage.yandexcloud.net
S3_ACCESS_KEY_ID=key_id
S3_SECRET_ACCESS_KEY=access_key
S3_BUCKET_NAME=bucket

# Packages config
PACKAGE_1_NAME="10 Generations"
